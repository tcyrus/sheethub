<div class="navbar-form input-group search-bar">
  <form action="<%= search_url %>" method="get">
    <div id="scrollable-dropdown-menu" class="has-feedback">
      <%= text_field_tag :q, params[:q], id: 'search_form', class: 'typeahead form-control', placeholder: 'Search for title' %>
